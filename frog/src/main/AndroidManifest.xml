<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="kr.co.ky.kozoltime">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_add_a_photo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_arrow_back"
        android:supportsRtl="true"
        android:theme="@style/Theme.Kozoltime"
        android:usesCleartextTraffic="true">
        <activity
            android:name="kr.co.ky.detail.DetailPage"
            android:exported="false" />
        <activity
            android:name="kr.co.ky.nicknamePackage.NickNameActivity"
            android:exported="false" />
        <activity
            android:name="kr.co.ky.login.LoginJoinActivity"
            android:exported="false" />
        <activity
            android:name="kr.co.ky.community.CommunityWriteActivity"
            android:exported="false" />
        <activity
            android:name="kr.co.ky.office.OfficeActivity"
            android:exported="false" />
        <activity
            android:name=".WriteActivity"
            android:exported="false" />
        <activity
            android:name="kr.co.ky.login.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="false" />
    </application>

</manifest>